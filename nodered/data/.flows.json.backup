    {
        "id": "em_parser_001",
        "type": "function",
        "z": "16c28e429371841e",
        "name": "Emergency Parser",
        "func": "var data = msg.payload;\nif (typeof data === 'string') {\n    try { data = JSON.parse(data); } catch (e) { return null; }\n}\n\nvar emType = data.type || 'unknown';\nvar location = data.location || 'unknown';\nvar severity = data.severity || 'none';\nvar active = data.active ? 1 : 0;\n\nmsg.measurement = 'emergencies';\nmsg.payload = [\n    {\n        active: active,\n        severity_level: severity === 'catastrophic' ? 3 : (severity === 'critical' ? 2 : (severity === 'warning' ? 1 : 0))\n    },\n    {\n        emergency_type: String(emType),\n        location: String(location),\n        severity: String(severity)\n    }\n];\nreturn msg;\n",
        "outputs": 1,
