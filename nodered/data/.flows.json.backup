    {
        "id": "thresh_parser_001",
        "type": "function",
        "z": "16c28e429371841e",
        "name": "Threshold Parser",
        "func": "var data = msg.payload;\nif (typeof data === 'string') {\n    try { data = JSON.parse(data); } catch (e) { return null; }\n}\n\n// Only handle threshold updates\nif (msg.topic !== \"City/update/thresholds\") return null;\n\nvar thresholds = data.thresholds;\nif (!thresholds) return null;\n\nvar messages = [];\nvar types = Object.keys(thresholds);\nfor (var i = 0; i < types.length; i++) {\n    var t = types[i];\n    messages.push({\n        measurement: \"thresholds\",\n        payload: [\n            { value: parseFloat(thresholds[t]) },\n            { type: String(t) }\n        ]\n    });\n}\nreturn [messages];\n",
        "outputs": 1,
