    {
        "id": "f4088b8a4cdfe38d",
        "type": "function",
        "z": "16c28e429371841e",
        "name": "JSON Parser",
        "func": "var data = msg.payload;\nif (typeof data === 'string') {\n    try {\n        data = JSON.parse(data);\n    } catch (e) {\n        node.warn(\"Error parsing JSON\");\n        return null;\n    }\n}\n\nvar topicParts = msg.topic.split('/');\nvar location = topicParts.length > 2 ? topicParts[2] : \"unknown\";\nvar sensorType = topicParts.length > 3 ? topicParts[3] : \"unknown\";\nvar unit = data.unit || \"value\";\n\nvar fields = {};\nfields[unit] = parseFloat(data.value) || 0;\n\nmsg.measurement = \"sensors\";\nmsg.payload = [\n    fields,\n    {\n        sensorid: String(data.sensorid || \"unknown\"),\n        location: String(location),\n        type: String(sensorType),\n        timestamp: String(data.timestamp ? new Date(data.timestamp) : new Date())\n    }\n];\nreturn msg;\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
